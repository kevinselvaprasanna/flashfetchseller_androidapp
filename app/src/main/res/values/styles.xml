<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--<item name="colorPrimary">#444D62</item>-->
        <!--<item name="colorPrimaryDark">#29303f</item>-->

        <item name="colorAccent">@color/ff_green</item>
        <item name="colorPrimary">#29303F</item>
        <item name="colorPrimaryDark">#29303f</item>
    </style>

    <!--<style name="AppTheme.NoActionBar" parent="AppTheme">-->
        <!--<item name="windowActionBar">false</item>-->
        <!--<item name="windowNoTitle">true</item>-->
    <!--</style>-->

    <style name="SplashTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/splash</item>
        <item name="colorPrimary">#29303f</item>
        <item name="colorPrimaryDark">#29303f</item>
        <item name="colorAccent">@color/ff_green</item>
    </style>

    <style name="PopupTheme" parent="AppTheme">
        <item name="android:popupBackground">#37474F</item>
    </style>

    <style name="ToolBarTheme" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorPrimary">#29303f</item>
        <item name="colorPrimaryDark">#29303f</item>
        <!--<item name="colorAccent">@color/colorAccent</item>-->
    </style>

    <style name="HDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="VDivider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="AppTheme.NoActionBar"/>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

</resources>
